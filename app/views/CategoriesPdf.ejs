<!DOCTYPE html>
<html lang="en">

<head>
  <title>Category PDF</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.3/moment.min.js"></script>
  <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <link href="~/Content/common/bootstrap.css" rel="stylesheet" **media="screen"** type="text/css" />
<link href="~/Content/common/bootstrap.min.css" rel="stylesheet" **media="screen"** type="text/css" />
<link href="~/Content/common/site.css" rel="stylesheet" **media="screen"** type="text/css" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.3/moment.min.js"></script>
  <script src="public/assets/plugins/jquery-steps/jquery.steps.min.js"></script>
  <script src="public/assets/plugins/parsleyjs/parsley.min.js"></script>
  <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
  <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
  <style>
    @media print{a[href]:after{content:none}} 
  table, th, td {
    border: 1px solid groove !important;  
    border-collapse: collapse;
  }
  </style>
</head>
<body>
  <div>

    <div class="col-md-12 mt-2">
                                 <div class="row">
                                     <!-- startt -->
                                     <div class="col-md-12">
                                         <div class="card-body cards" style="height: 400px; padding-top: 0%;">
                                             <div class="Row">
                                             <div class="card-body cards" id="card_id" style="border-bottom: 1px solid #dadada;">
                                                <h2 style="font-size:23px;font-weight:600;font-family: 'Libre Baskerville', serif;">
                                                 <div class="textSize name" style="display:-webkit-flex;-webkit-align-items:flex-end;    
                                                 width: 50%;text-align: left; font-family:'Comfortaa';font-style: normal;font-weight: 700;font-size: 23px;line-height:36px;color: #000000;">

                                                   <a href="https://livesanjivani.com/" class="tool_tip m-l-20"
                                                    data-toogle="tooltip" title="Edit"> <img src="https://live-sanjivani.s3.us-east-2.amazonaws.com/icons/app-logo.png" alt="Live Sanjivani" 
                                                   width="35px" height="35px">&nbsp;<span>Live Sanjivani</span> </a>
                                                  
                                                 </div>
                                             </h2></div>
                                  <table class="table" style="width:90%; border: 1px solid groove;margin-left: 30px !important">
                               <thead>
                               <tr>
          <!-- <th scope="col">Sr.</th> -->
          <th scope="col">Field</th>
          <th scope="col">Value</th>
          <th scope="col">Date And Time</th>

                               </tr>
                             </thead>
                           <tbody>
        <% for(var i=0; i < CategoryData.length; i++) { %>
        <tr>
          <!-- <td rowspan="<%= CategoryData[i].user_subcategories_values.length%>"><%= i+1 %></td> -->
          <td rowspan="<%= CategoryData[i].user_subcategories_values.length%>" ><%= i+1 %>. <%= CategoryData[i].name %></td>
          <% for(var j=0; j < CategoryData[i].user_subcategories_values.length; j++) { %>
          <% if(j==0){%>
          <td>
            <%= CategoryData[i].user_subcategories_values[j].value?CategoryData[i].user_subcategories_values[j].value:0 %>
          </td>
          <td>

            <%=  moment(CategoryData[i].user_subcategories_values[j].createdAt)
          .utc()
          .format("YYYY-MM-DD HH:mm:ss")%>
          </td>
        </tr>
        <% }else if(j==1){%>
        <tr>
          <td>
            <%= CategoryData[i].user_subcategories_values[j].value?CategoryData[i].user_subcategories_values[j].value:0 %>
          </td>
          <td>
            <%=  moment(CategoryData[i].user_subcategories_values[j].createdAt)
          .utc()
          .format("YYYY-MM-DD HH:mm:ss")%>
          </td>
        </tr>

        <% }else if(j==(CategoryData[i].user_subcategories_values-1)){%>
        <tr>
          <td>
            <%= CategoryData[i].user_subcategories_values[j].value?CategoryData[i].user_subcategories_values[j].value:0 %>
          </td>
          <td>
            <%=  moment(CategoryData[i].user_subcategories_values[j].createdAt)
          .utc()
          .format("YYYY-MM-DD HH:mm:ss")%>
          </td>
        </tr>

        <% }else{ %>
        <tr>
          <td>
            <%= CategoryData[i].user_subcategories_values[j].value?CategoryData[i].user_subcategories_values[j].value:0 %>
          </td>
          <td>
            <%=  moment(CategoryData[i].user_subcategories_values[j].createdAt)
          .utc()
          .format("YYYY-MM-DD HH:mm:ss")%>
          </td>
        </tr>
        <% } %>
        <% } %>
        </tr>

        <% } %>

      </tbody>
    </table>
  </div>
</body>
</div></div></div></div></div>
</html>
